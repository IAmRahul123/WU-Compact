{
  "name": "WU",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "setup": "cross-env gulp prepare-config --env=$ENV --country=$COUNTRY && husky install",
    "setup-uat": "cross-env gulp prepare-config --env=UAT --country=IN",
    "android": "npm run setup-uat && npx react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "@reduxjs/toolkit": "^2.8.2",
    "i18next": "^25.2.1",
    "react": "19.0.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.79.2",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "gulp": "^5.0.1",
    "gulp-merge-json": "^2.2.1",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "yargs": "^18.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
